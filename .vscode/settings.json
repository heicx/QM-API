// 将设置放入此文件中以覆盖默认值和用户设置。
{
    "files.exclude": {
      // exclude .js and .js.map files, when in a TypeScript project
      "node_modules": false
    },
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "boundary",
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
    "files.insertFinalNewline": true
  }
  